public class VectorRunner {
    public static void main(String[] args) {
        Vector vector1=new Vector(2,4,5);
        Vector vector2=new Vector(3,4,3);

        System.out.println(Vector.getSumVectors(vector1,vector2));
        System.out.println(Vector.getSkalarVectors(vector1,vector2));
        System.out.println(Vector.getProizVectors(vector1,vector2));
    }
}
