import java.util.ArrayList;

public class Network {
   private final String name="ProgStar";
    private static int [] listPhone=new int[10];
    private static int count=0;
    private static int  numberToSend;


    public static void addPhone(int number){
      listPhone[count]=number;
      count++;
    }

    public static boolean isCallProcessing(int number){
        boolean is=false;
        for (int i = 0; i <count; i++) {
            if(listPhone[i]==number){
                is=true;
                numberToSend=number;

            }
        }
        return is;
    }

    public static int toSendCall(){
        return numberToSend;
    }




}
